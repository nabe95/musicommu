<div class="container">
  <div class="row my-2">
    <div class="col-md-8">
      <h4>ユーザー一覧</h4>
      <table class='table'>
          <tr>
            <th></th>
            <th>名前</th>
            <th>プロフィール</th> 
            <th>年齢</th>
            <th>住所</th>
            <th></th>
          </tr>
          <% @users.each do |user| %>
            <tr>
              <td><%= image_tag user.get_profile_image, size: '50x50' %></td>
              <td><%= user.name %></td>
              <td><%= user.introduction %></td>
              <td><%= user.age %></td>
              <td><%= user.prefectures %></td>
              <td><%= link_to '詳細', user %></td>
            </tr>
          <% end %>
      </table>
    </div>
    </div>
</div>