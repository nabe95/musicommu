<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>投稿時間</th>
      <th>タイトル</th>
      <th></th>
    </tr>
  </thead>
  <tbody>
    <% posts.each do |post| %>
    <tr>
      <td><%= post.created_at.strftime("%Y/%m/%d %H:%M")%></td>
      <td><%= link_to post.title %></td>
      <td><%= link_to(post.user) do %>
        <%= image_tag post.user.get_profile_image, size:'50x50' %>
        <% end %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>